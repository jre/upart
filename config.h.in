/* -*-c-*- */
#ifndef HDR_UP_CONFIG
#define HDR_UP_CONFIG

#undef PACKAGE_NAME
#undef PACKAGE_VERSION

/* linux is great */
#undef _GNU_SOURCE
#undef _FILE_OFFSET_BITS
#undef _LARGE_FILES

/* standard headers */
#undef HAVE_SYS_TYPES_H
#undef HAVE_SYS_PARAM_H
#undef HAVE_SYS_IOCTL_H
#undef HAVE_SYS_STAT_H
#undef HAVE_DIRENT_H
#undef HAVE_INTTYPES_H
#undef HAVE_STDINT_H
#undef HAVE_UNISTD_H

/* standard functions */
